<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.board.dao.IDao">
<select id="listDao" resultType="com.board.domain.M_CategoryVo">
		SELECT * FROM M_CATEGORY ORDER BY category_Id DESC
	</select>
	
	
	<insert id="writeDao">
		INSERT INTO M_CATEGORY (CATEGORY_ID, MOVIE_CATEGORY) VALUES (#{param1}, #{param2})
		
	</insert>
	
	
	<delete id="deleteDao">
	DELETE FROM M_CATEGORY WHERE CATEGORY_ID= #{param}
	</delete>
	
	
</mapper>
